{
  "template": {
    "prefix": "template",
    "body": [
      "import React, { memo, ReactNode, FC } from 'react';",
      "",
      "interface IProps {",
      "\tchildren?: ReactNode;",
      "}",
      "",
      "const ${1:Template}: FC<IProps> = () => {",
